package oopIntro;

public class Product {
	
	//Constructor
	public Product() {
		System.out.println("Ben Çalýþtým");
	}
	
	//Overloading
	public Product(int id, String name, double unitPrice, String detail) {
		this.id = id;
		this.name = name;
		this.unitPrice = unitPrice;
		this.detail = detail;
	}
	
	int id;
	String name;
	double unitPrice;
	String detail;
	

}
